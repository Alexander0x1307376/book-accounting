  "license": "MIT",
  "scripts": {
    "dev": "yarn nodemon --require dotenv/config index.ts --inspect",
    "compile": "tsc --watch",
    "seed:config": "yarn ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "yarn ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "yarn ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "yarn ts-node ./node_modules/typeorm/cli.js schema:sync",
    "test": "nyc ./node_modules/.bin/_mocha 'test/**/*.test.ts'"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-query-parser": "^1.3.2",
    "express-validator": "^6.13.0",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "pg": "^8.7.1",
    "supertest": "^6.1.6",
    "ts-custom-error": "^3.2.0",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/chai-http": "^4.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/lodash": "^4.14.177",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
